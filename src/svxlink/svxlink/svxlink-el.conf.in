[GLOBAL]
LOGICS=ReflectorLogic,SimplexLogic
CFG_DIR=svxlink.d
TIMESTAMP_FORMAT=%c
LINKS=AllLink

[SimplexLogic]
TYPE=Simplex
EVENT_HANDLER=/usr/share/svxlink/events.tcl
CALLSIGN=EL
DEFAULT_LANG=en_US
RX=NONE
TX=NONE
MODULES=ModuleEchoLink

[AllLink]
CONNECT_LOGICS=ReflectorLogic,SimplexLogic
DEFAULT_ACTIVE=1
TIMEOUT=0

[ReflectorLogic]
TYPE=Reflector
CALLSIGN=EL
HOSTS=127.0.0.1
AUTH_KEY=abc123
HOST_PORT=5300
AUDIO_CODEC=OPUS
JITTER_BUFFER_DELAY=0
MONITOR_TGS=999
DEFAULT_TG=999
TG_SELECT_TIMEOUT=-1
EVENT_HANDLER=/usr/share/svxlink/events.tcl